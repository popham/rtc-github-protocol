{
  "name": "rtc-github-protocol",
  "version": "0.0.0",
  "description": "Signaling protocol for rtc-github",
  "scripts": {
    "clean": "rm -rf node/*.capnp.d/ node/*.capnp amd/*.capnp.d/ amd/*.capnp",
    "compileMySchema": "capnp compile -ojs:amd *.capnp & cp *.capnp amd & wait",
    "nfy": "nfy --output=node --src-prefix=amd amd/*.capnp.d/*.js & cp *.capnp node & wait",
    "build": "npm run rebuild",
    "rebuild": "npm run clean && npm run compileMySchema && npm run nfy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popham/rtc-github-protocol.git"
  },
  "author": "Tim Popham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/popham/rtc-github-protocol/issues"
  },
  "homepage": "https://github.com/popham/rtc-github-protocol",
  "devDependencies": {
    "capnp-js-plugin": "0.2.5",
    "nfy": "0.0.1"
  }
}
